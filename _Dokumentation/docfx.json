{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "DotNetTools.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ],
		  "src": "../DotNetTools/DotNetTools"
        }
      ],
      "properties": {
        "TargetFrameworks": "netstandard2.0"
      },
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
		  "api/**.yml",
          "api/index.md",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "dest": "docs",
	"globalMetadata": {
		"_appTitle": "AppFrameDotNet.DotNetTools",
		"_appFooter": "© 2020 Dataport AöR",
		"_appLogoPath": "images/combined_logos.png",
		"_appFaviconPath": "images/appFrame.png"
	},
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "statictoc"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}